# AISIS Credentials
AISIS_USERNAME=your_aisis_username
AISIS_PASSWORD=your_aisis_password

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
DATA_INGEST_TOKEN=your_data_ingest_token

# Optional: Override term for manual scraping (auto-detected if not set)
# APPLICABLE_PERIOD=2024-2  # Legacy - still supported
# AISIS_TERM=2025-1         # Preferred - takes precedence over APPLICABLE_PERIOD

# Optional: Performance tuning for Supabase sync
# SUPABASE_CLIENT_BATCH_SIZE=2000  # Default: 2000 records per Edge Function call
                                   # Larger = fewer HTTP requests, faster sync
                                   # Smaller = more granular progress, safer for timeouts

# Optional: Google Sheets integration
# GOOGLE_SERVICE_ACCOUNT={"type":"service_account",...}
# SPREADSHEET_ID=your_spreadsheet_id
